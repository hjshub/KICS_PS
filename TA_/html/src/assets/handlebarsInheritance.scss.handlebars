{{#spritesheet}}
  @mixin icon { display: inline-block; background-image: url($sprite-url); background-repeat: no-repeat; background-size:
  {{px.width}}
  {{px.height}}
  } .icon {font-size:0;} .icon::before {@include icon; content:'';} .icon + em, .icon + b { margin-left:10px;} .column > .icon +
  em, .column > .icon + b { margin-left:0; }
{{/spritesheet}}

{{#sprites}}
  @mixin icon-{{name}}{ width:{{px.width}}; height:{{px.height}}; background-position:
  {{px.offset_x}}
  {{px.offset_y}}; } .icon-{{name}}::before { @include icon-{{name}}
  }
{{/sprites}}